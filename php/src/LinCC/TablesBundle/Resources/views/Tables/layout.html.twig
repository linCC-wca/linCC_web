{% extends "::base.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets '@LinCCTablesBundle/Resources/public/css/style.css' filter='?uglifycss' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts
        '@hinclude'
        '@LinCCTablesBundle/Resources/public/js/script.js'
        filter='?uglifyjs' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block body %}
    {{ include('LinCCTablesBundle:Tables:nav.html.twig') }}
    {% block content %}{% endblock %}
{% endblock %}
